@model Gears.ViewModels.HomeViewModel


    <div class="contact">
        <h4>أتصل بنا </h4>
        <ul class="list-unstyled nopadding ">
            <li>
                <i class="fas fa-map-marked-alt"></i>   @Model.ContactUs.Address
            </li>
            <li>
                <a href="01228595201">
                    <i class="fas fa-phone   text-center"></i> @Model.ContactUs.Phone
                </a>
            </li>
            <li>
                <a href="@Model.ContactUs.Gmail">
                    <i class="far fa-envelope"></i> @Model.ContactUs.Gmail
                </a>
            </li>
            <li>
                <a href="@Model.ContactUs.Url">
                    <i class="far fa-envelope"></i> @Model.ContactUs.Url
                </a>
            </li>
        </ul>
    </div>
