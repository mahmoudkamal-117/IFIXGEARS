<body>



    <!-- start navbar -->
    <nav class="navbar">
        <div class="grid">
            <div class="brand-togle">
                <a class=" brand" href="#">
                    <img class="w-100" src="~/image/i-fix.png">
                </a>
                <button class="togle  d-lg-none d-sm-block" id="toggle">
                    <span></span>
                    <span></span>
                    <span></span>
                </button>
            </div>

            <div class="flex-navebar" id="fixed-navbar">
                <ul class="list-unstyled links text-center" id="links">
                    <li><a class="active" href="#"> الرئيسية </a></li>
                    <li><a href="#about-us">من نحن</a></li>
                    <li><a href="#our-work">اهم الاعمال </a></li>
                    <li><a href="#see-us">شـاهـدنـا</a></li>
                    <li><a href="#reviews">أراء العملاء</a></li>

                </ul>
                <ul class="list-unstyled icons text-left">
                    <li> <a href="#"><i class="fab fa-facebook"></i></a></li>
                    <li> <a href="#"><i class="fab fa-youtube"></i></a></li>
                    <li> <a href="#"><i class="fab fa-instagram"></i></a></li>
                </ul>
            </div>
        </div>
    </nav>

    <!-- end navbar -->


   <div>
       @RenderBody()
   </div>
    
    <footer class="footer ">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-6">
                   @await Html.PartialAsync("_ContactUsIndex")
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="follow-us">
                        <h4>تابعنا </h4>
                        <ul class="list-unstyled icons  nopadding d-flex">
                            <li> <a href="#"><i class="fab fa-facebook"></i></a></li>
                            <li> <a href="#"><i class="fab fa-youtube"></i></a></li>
                            <li> <a href="#"><i class="fab fa-instagram"></i></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <h4>ساعات العمل </h4>
                    <div class="hours-work">
                        <ul class="list-unstyled nopadding">
                            <li class="d-flex">
                                <span class="day">السبت</span>
                                <span class="time text-left"> 6 : 10</span>
                            </li>

                            <li class="d-flex">
                                <span class="day">الاحد</span>
                                <span class="time text-left"> 6 : 10</span>
                            </li>
                            <li class="d-flex">
                                <span class="day">الاتنين  </span>
                                <span class="time"> 6 : 10</span>
                            </li>
                            <li class="d-flex">
                                <span class="day"> الثلاثاء</span>
                                <span class="time text-left"> 6 : 10</span>
                            </li>
                            <li class="d-flex">
                                <span class="day">الاربعاء </span>
                                <span class="time text-left"> 6 : 10</span>
                            </li>
                            <li class="d-flex">
                                <span class="day">الخميس </span>
                                <span class="time text-left"> 6 : 10</span>
                            </li>
                            <li class="d-flex">
                                <span class="day">الجمعة: </span>
                                <span class="time"> close</span>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <h4>موقعنا </h4>
                    <div class="map">
                        <div class="contianer-map">
                            <iframe frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?width=520&amp;height=400&amp;hl=en&amp;q=soudan%20cairo+()&amp;t=&amp;z=12&amp;ie=UTF8&amp;iwloc=B&amp;output=embed"></iframe> <a href='https://www.symptoma.ae/ar/info/covid-19#info'>COVID</a>
                            <script type='text/javascript' src='https://embedmaps.com/google-maps-authorization/script.js?id=c0050f61ff8d07b754b8f12c57cde5f5f6a00ffb'></script>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="copyright text-center">
            <p>جميع الحقوق محفوظة  &copy; 2020</p>
        </div>
    </footer>






    <script src="~/js/jquery-3.4.1.min.js"></script>
    <script src="~/js/jquery.event.move.js"></script>
    <script src="~/js/jquery.twentytwenty.js"></script>
    <script src="~/js/bootstrap.min.js"></script>

    <script src="~/js/main.js"></script>
</body>
</html>